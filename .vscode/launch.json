{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug ART_tests",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/art_tests.exe",
            "args": [
                "--gtest_filter=ARTTest.SingleSearch",
                "--gtest_break_on_failure"
            ],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "C:/mingw64/bin/gdb.exe",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}
